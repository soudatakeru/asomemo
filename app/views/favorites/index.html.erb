<div class="favorite__main">
  <h2>お気に入りのイベント</h2>
      <% @favorites.each do |favorite| %>
        <% event = Event.find(favorite.event_id) %>
          <div class="name3"><%= link_to event.name, event_path(event) %></div>
          <div class="name2"><%= link_to "参加中のイベントから削除",favorite_path(favorite), method: :delete %></div>
      <% end %>
</div>